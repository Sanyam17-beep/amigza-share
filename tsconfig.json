{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
   "target": "esnext",
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "strict": true,
    "module": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
